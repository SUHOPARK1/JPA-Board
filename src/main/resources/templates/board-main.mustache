{{>layout/header}}
<br/>
<center><h2>게시판</h2></center>
<div class="col-md-12">
    <div class="row">
        <div class="col-md-6">
            <a href="/" role="button" class="btn btn-primary">홈으로</a>
            <a href="/board/save" role="button" class="btn btn-primary">작성하기</a>
        </div>
    </div>
    <br/>
    <table class="table table-horizontal table-bordered">
        <thead class="thead-strong">
        <tr>
            <th>번호</th>
            <th>제목</th>
            <th>ID</th>
            <th>작성날짜</th>
            <th>수정날짜</th>
            <th>조회수</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#board}}
        <tr>
            <td>{{bdId}}</td>
            <td><a href="/board/detail/{{bdId}}">{{bdSubject}}</a></td>
            <td>{{createdId}}</td>
            <td>{{createDate}}</td>
            <td>{{modifiedDate}}</td>
            <td>{{bdViewCnt}}</td>
        </tr>
        {{/board}}
        </tbody>
    </table>
</div>
{{>layout/footer}}